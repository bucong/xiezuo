/* pages/process/process.wxss */
page{
  background: #f5f7f9;
}
.head{
  text-align: center;
  height: 280rpx;
  color: #fff;
  position: relative;
}
.head .bg{
  width: 750rpx;
  height: 280rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
}
.head view{
  font-size: 44rpx;
  line-height: 60rpx;
  padding: 46rpx 0;
}
.head .proStatus{
  width: 40rpx;
  height: 40rpx;
}
.list-box{
  margin: 66rpx 45rpx;
}
.list{
  width: 660rpx;
  height: 100rpx;
  font-size: 28rpx;
  border-radius: 12rpx;
  overflow: hidden;
  background: #fff;
  margin-bottom: 40rpx;
}
.list image{
  width: 12rpx;
  height: 20rpx;
  margin-right: 20rpx;
}
.list .time{
  width: 62rpx;
  height: 64rpx;
  padding: 18rpx 30rpx;
  background: #3370FF;
  color: #fff;
  text-align: center;
  line-height: 32rpx;
}
.list .name{
  width: 468rpx;
  font-size: 26rpx;
}
.list-box .finish .time{
  color: #B3B3B3;
  background: #E6E6E6;
}
.list-box .finish .name{
  color: #999;
}